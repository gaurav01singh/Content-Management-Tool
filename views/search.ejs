<div class="author">
  <h1 class="author_heading">Search Results</h1>
</div>
<section class="articals">
  <ul class="articles_ul">
    <% data.forEach(post => { %>
    <li>
      <a href="/post/<%= post._id %>"
        ><span> <%= post.title %></span>
        <span class="articles-list_date"
          ><%= post.createdAt.toDateString() %></span
        ></a
      >
    </li>
    <% }) %>
  </ul>
</section>
